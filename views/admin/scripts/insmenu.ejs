<script>
let formCreate = document.querySelector('#modal-create form');

  formCreate.addEventListener('submit', e=>{
    e.preventDefault();
    let formData = new FormData(formCreate);

    fetch('/admin/menus', {
      method: 'post',
      body: formData
    }).then(response=>{response.json()
      .then(json=>{
        window.location.reload();
      });
    })

});
</script>